@article{chillarege_orthogonal_1992,
  title =        {Orthogonal {Defect} {Classification} - {A} {Concept}
                  for {In}-{Process} {Measurements}},
  volume =       18,
  url =
                  {http://search.proquest.com.ezproxy.library.tufts.edu/docview/195567951?accountid=14434},
  abstract =     {A description is presented of orthogonal defect
                  classification (ODC), a concept that enables
                  in-process feedback to developers by extracting
                  signatures on the development process from
                  defects. The ideas are evolved from an earlier
                  finding that demonstrates the use of semantic
                  information from defects to extract cause-effect
                  relationships in the development process. This
                  finding is leveraged to develop a systematic
                  framework for building measurement and analysis
                  methods. The use of the defect type distribution is
                  illustrated to measure the progress of a product
                  through a process. In addition, the use of the
                  defect trigger distribution is illustrated to
                  evaluate the effectiveness and eventually the
                  completeness of verification processes such as
                  inspection or testing. Sample results are provided
                  from pilot projects using ODC. The analysis opens
                  the doors to a wide variety of analysis techniques
                  for providing effective and fast feedback based on
                  the concepts of ODC.},
